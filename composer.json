{
  "name": "uendelsilveira/laravel-tenant-core",
  "description": "Infrastructure-only multi-tenant multi-database package for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
	{
	  "name": "Uendel Silveira",
	  "email": "uendelsilveira@gmail.com"
	}
  ],
  "require": {
	"php": "^8.5",
	"illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
	"psr-4": {
	  "UendelSilveira\\TenantCore\\": "src/"
	},
	"files": [
	  "src/Helpers/helpers.php"
	]
  },
  "extra": {
	"laravel": {
	  "providers": [
		"UendelSilveira\\TenantCore\\Providers\\TenantServiceProvider"
	  ]
	}
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
